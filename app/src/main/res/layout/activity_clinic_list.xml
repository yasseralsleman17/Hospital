<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".User.ClinicList">


    <ImageView
        android:id="@+id/ortho"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"

        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cardiolgy"
        app:srcCompat="@drawable/ortho" />

    <ImageView
        android:id="@+id/obgyn"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"

        android:layout_marginTop="20dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/ortho"
        app:layout_constraintTop_toBottomOf="@+id/neuology"
        app:srcCompat="@drawable/obgyn" />

    <ImageView
        android:id="@+id/pulmonary"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="40dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageButton"
        app:srcCompat="@drawable/pulmonary" />

    <ImageView
        android:id="@+id/dermatology"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="40dp"

        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/pulmonary"
        app:layout_constraintTop_toBottomOf="@+id/cardView"
        app:srcCompat="@drawable/dermatology" />

    <ImageView
        android:id="@+id/dental"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:scaleType="centerCrop"

        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/pulmonary"
        app:srcCompat="@drawable/dental" />

    <ImageView
        android:id="@+id/pediatric"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"

        android:layout_marginTop="16dp"
        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/dental"
        app:layout_constraintTop_toBottomOf="@+id/dermatology"
        app:srcCompat="@drawable/pediatric" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="57dp"
        android:layout_height="54dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="16dp"
        android:background="@color/white"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_before" />

    <androidx.cardview.widget.CardView
        android:id="@+id/cardView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="@dimen/_30sdp"
        android:layout_marginTop="@dimen/_15sdp"
        app:cardElevation="@dimen/_5sdp"
        app:layout_constraintStart_toEndOf="@+id/imageButton"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="@dimen/_5sdp"
            android:text="CLINIC LIST"
            android:textSize="30dp"
            tools:layout_editor_absoluteY="43dp" />
    </androidx.cardview.widget.CardView>


    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="62dp"
        android:layout_height="50dp"
        android:layout_marginStart="56dp"
        android:layout_marginTop="20dp"
        app:layout_constraintStart_toEndOf="@+id/cardView"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_def" />

    <ImageView
        android:id="@+id/cardiolgy"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="16dp"

        android:scaleType="centerCrop"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/dental"
        app:srcCompat="@drawable/cardiolgy" />

    <ImageView
        android:id="@+id/neuology"
        android:layout_width="@dimen/_100sdp"
        android:layout_height="@dimen/_100sdp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="16dp"

        android:scaleType="centerCrop"
        app:layout_constraintStart_toEndOf="@+id/cardiolgy"
        app:layout_constraintTop_toBottomOf="@+id/pediatric"
        app:srcCompat="@drawable/neuology" />


</androidx.constraintlayout.widget.ConstraintLayout>